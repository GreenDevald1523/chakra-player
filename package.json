{
  "name": "chakra-player",
  "version": "0.2.1",
  "private": false,
  "licence": "MIT",
  "homepage": "https://greendevald1523.github.io/player/",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@chakra-ui/react": "^2.2.1",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "framer-motion": "^6.2.9",
    "gh-pages": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^3.0.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks",
      "plugin:react/jsx-runtime",
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/require-default-props": [
        "error",
        {
          "functions": "defaultArguments"
        }
      ]
    }
  },
  "prettier": {
    "arrowParens": "always",
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "React player, made with [`Chakra UI`](https://chakra-ui.com/), can play the audio that you push in it.",
  "main": "dist/index.js",
  "module": "dist/index,js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreenDevald1523/player.git"
  },
  "keywords": [
    "chakra-player"
  ],
  "author": "moonlancer",
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GreenDevald1523/player/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "2.7.1"
  }
}
